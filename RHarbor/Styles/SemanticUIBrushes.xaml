    <SolidColorBrush x:Key="BrownBrush" Color="#A5673F"/>
    <SolidColorBrush x:Key="GreyBrush" Color="#767676"/>
    <SolidColorBrush x:Key="BlackBrush" Color="#1B1C1D"/>
